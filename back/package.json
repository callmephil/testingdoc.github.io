    "@babel/register": "^7.0.0",
    "better-sqlite3": "^5.4.0",
    "body-parser": "^1.18.3",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.5",
    "dotenv": "^7.0.0",
    "express": "^4.16.4",
    "express-jwt": "^5.3.1",
    "express-session": "^1.15.6",
    "helmet": "^3.15.0",
    "http-errors": "^1.7.1",
    "jwks-rsa": "^1.3.0",
    "multer": "^1.4.1",
    "nodemon": "^1.18.6",
